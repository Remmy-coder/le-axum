{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug `Test App`",
      "type": "codelldb",
      "request": "launch",
      "program": "${cargo:program}",
      "cargo": {
        "args": [
          "build"
        ],
        "problemMatcher": "$rustc"
      },
      "cwd": "${workspaceFolder}",
      "sourceLanguages": [
        "rust"
      ],
      "stopOnEntry": false
    }
  ]
}
